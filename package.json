{
    "name": "gs-grid",
    "version": "0.0.1",
    "description": "A new grid for evolving web",
    "main": "index.ts",
    "repository": "https://github.com/ganesh1004/gs-grid.git",
    "author": "velankiganesh@gmail.com",
    "license": "MIT",
    "private": true,
    "scripts": {
        "tsc:watch": "tsc --watch",
        "typedoc": "typedoc",
        "tsc": "tsc",
        "webpack": "webpack",
        "rm": "rm",
        "docs": "typedoc --out ./demo/docs --readme none --mode file",
        "build:src": "tsc && webpack",
        "prestart": "yarn build && yarn docs",
        "build": "yarn build:src",
        "start": "webpack-dev-server",
        "clean": "yarn rm -rf dist && yarn rm -rf demo/docs && yarn rm -rf demo/index.bundle.js",
        "dev": "yarn build && yarn docs && concurrently --kill-others \"yarn tsc:watch\" \"yarn webpack --watch\" \"yarn webpack-dev-server\""
    },
    "husky": {
        "hooks": {}
    },
    "dependencies": {
        "rxjs": "~6.6.2"
    },
    "devDependencies": {
        "css-loader": "^4.3.0",
        "husky": "~4.3.0",
        "rmrf": "~2.0.2",
        "style-loader": "^1.2.1",
        "ts-loader": "^8.0.3",
        "typedoc": "^0.19.1",
        "typescript": "^3.8.3",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "concurrently": "^5.3.0"
    }
}
